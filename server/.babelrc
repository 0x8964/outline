{
  "presets": [
    "@babel/preset-react",
    "@babel/preset-typescript",
    [
      "@babel/preset-env",
      {
        "corejs": {
          "version": "2",
          "proposals": true
        },
        "targets": {
          "node": "12"
        },
        "useBuiltIns": "usage"
      }
    ]
  ],
  "plugins": [
    "transform-class-properties"
  ]
}